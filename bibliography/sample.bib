doi = {10.1080/07391102.2014.998710},
file = {:Users/anupadkh/Library/Application Support/Mendeley Desktop/Downloaded/Xiao et al. - 2015 - IDrug-Target Predicting the interactions between drug compounds and target proteins in cellular networking via b(2).pdf:pdf},
issn = {15380254},
journal = {Journal of Biomolecular Structure and Dynamics},
keywords = {NCR,SMOTE,chous PseAAC,iDrug-Chl,iDrug-Ezy,iDrug-GPCR,iDrug-NR,molecular fingerprints,optimized training data-set,target-jackknife validation},
mendeley-groups = {Protein{\_}Drug{\_}Interaction/DataSet,Protein{\_}Drug{\_}Interaction},
number = {10},
pages = {2221--2233},
pmid = {25513722},
title = {{IDrug-Target: Predicting the interactions between drug compounds and target proteins in cellular networking via benchmark dataset optimization approach}},
