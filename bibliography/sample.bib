issn = {20452322},
journal = {Scientific Reports},
mendeley-groups = {Protein{\_}Drug{\_}Interaction},
number = {1},
pages = {1--12},
pmid = {28407034},
publisher = {Springer US},
title = {{Reciprocal Perspective for Improved Protein-Protein Interaction Prediction}},
