mendeley-groups = {Protein{\_}Drug{\_}Interaction/Algorithms},
number = {1},
pages = {1--12},
publisher = {BMC Bioinformatics},
title = {{Predicting protein-ligand binding residues with deep convolutional neural networks}},
volume = {20},
year = {2019}
}
